    <main class="main-wrap">
        <header class="main-header navbar">
            <div class="col-search">
                
            </div>
            <div class="col-nav">
                <button class="btn btn-icon btn-mobile me-auto" data-trigger="#offcanvas_aside"> <i class="material-icons md-apps"></i> </button>
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link btn-icon darkmode" href="#"> <i class="material-icons md-nights_stay"></i> </a>
                    </li>
                    <li class="dropdown nav-item">
                        <a class="dropdown-toggle" data-bs-toggle="dropdown" href="#" id="dropdownAccount" aria-expanded="false"> <img class="img-xs rounded-circle" src="../admin-assets/imgs/people/avatar2.jpg" alt="User"></a>
                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="dropdownAccount">
                            <a class="dropdown-item" href="#"><i class="material-icons md-settings"></i>Account</a>
                            <div class="dropdown-divider"></div>
                            <a class="dropdown-item text-danger" href="/admin/logout"><i class="material-icons md-exit_to_app"></i>Logout</a>
                        </div>
                    </li>
                </ul>
            </div>
        </header>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Banner Management </h2>
                    
                </div>

                
            </div>
            <form action="/admin/banner" method="post" onsubmit="return checkCoupon()" enctype="multipart/form-data">
                <div class="mb-4">
                    <div class="row">
                        <br>
                        <div class="col-lg-4" >
                            <div class="mb-4" >
                                <label class="form-label">Main heading</label>
                                <input type="text" class="form-control"  name="h1"  style="background-color: rgb(211, 204, 204);" id="h1">
                                <div id="errorh1" style="color: red;"></div>
                            </div>
                        </div>

                        <div class="col-lg-4">
                            <div class="mb-4">
                                <label class="form-label">Sub head 1</label>
                                <input type="text" class="form-control"name="h2" id="h2"  style="background-color: rgb(211, 204, 204);">
                                <div id="errorh2" style="color: red;"></div>
                            </div>
                        </div>

                        <div class="col-lg-4">
                            <div class="mb-4">
                                <label class="form-label">Sub head 2</label>
                                <input type="text" class="form-control"  name="h3" id="h3"  style="background-color: rgb(211, 204, 204);">
                                <div id="errorh3" style="color: red;"></div>
                            </div>
                        </div>

                        <div class="col-lg-4">
                            <div class="mb-4">
                                <label class="form-label">Sub head 3</label>
                                <input type="text" class="form-control"  name="p1" id="p1"  style="background-color: rgb(211, 204, 204);">
                                <div id="errorp1" style="color: red;"></div>
                            </div>
                        </div>

                        <div class="col-lg-4" >
                            <div class="mb-4" >
                                <label class="form-label">Discription</label>
                                <input type="text" class="form-control"   name="discription"  style="background-color: rgb(211, 204, 204);" id="cdisc">
                                <div id="errordisc" style="color: red;"></div>
                            </div>
                        </div>

                        <div class="col-lg-4" >
                            <div class="mb-4" >
                                <div class="input-upload">
                                    <img src="/admin-assets/imgs/theme/upload.svg" alt="">
                                    <input class="form-control form-control-lg" type="file" name="image">  
                                    <div id="errorImage" style="color: red;"></div>                                              
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="text-center">
                    <button type="submit" class="btn btn-primary"><i
                            class="text-muted material-icons md-post_add"></i>Publish Banner</button>
                </div>
            </form>
        
            <div class="col-lg-12">
                <div class="card mb-4">
                    <div class="card-header">
                        <h4>BANNER DETAILS</h4>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Sl.No</th>
                                        <th>Image</th>
                                        <th>Description</th>
                                        <th>Status</th>
                                        <th>Action</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%for(let i=0; i< banner.length; i++){%>
                                        <tr>
                                            <td>
                                               <span style="color: black; font-weight: bolder;"><%=i+1%></span> 
                                            </td>
    
                                            <td class="image product-thumbnail">
                                                <img src="/admin-assets/imgs/banner/<%=banner[i].Image%>"   alt="Banner Image" class="card-img-top">
                                            </td>
                                            <td>
                                                <span style="color: black; font-weight: bolder;"><%=banner[i].h1%><br><%=banner[i].h2%><br></span> 
                                            </td>
                                            <td>
                                                <span style="color: green; font-weight: bolder" id="statusspan<%= banner[i]._id %>" data-banner-id="<%= banner[i]._id %>"><%=banner[i].status%></span>  
                                            </td>
                                            <td>
                                              <button onclick="Changestatus('<%=banner[i]._id%>')" class="btn-success btn-md">Change Status</button>
                                            </td>
                                            <td>
                                                <a href="/admin/editBanner?id=<%=banner[i]._id%>"><button class="btn-warning btn-md">Edit</button></a>
                                            </td>
                                        </tr>
                                        <%}%>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

               
        </section> <!-- content-main end// -->
        
    </main>
    




