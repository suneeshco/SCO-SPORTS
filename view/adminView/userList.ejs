            <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Seller</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th>Registered</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                <%for(let i=0;i<user.length;i++){%>
                                <tr>
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            <div class="left">
                                                <img src="data:image/png;base64," class="img-sm img-avatar" alt="Userpic">
                                            </div>
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title"><%= user[i].name%></h6>
                                                
                                            </div>
                                        </a>
                                    </td>
                                    <td><%=user[i].email%></td>
                                    <%if(user[i].status==true){%>
                                    <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    <%}else{%>
                                        <td><span class="badge rounded-pill alert-danger">Blocked</span></td>
                                        <%}%>
                                    <%const time= new Date(user[i].date)%>
                                    <%const timeForm= time.toLocaleDateString()%>
                                    <td><%=timeForm%></td>
                                    <td class="text-end">
                                        
                                        <%if(user[i].status==true){%>
                                        <a href="/admin/users/userStatusBlock?id=<%=user[i]._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15 " onclick="return confirmDelete('/admin/users/userStatusBlock?id=<%=user[i]._id%>','block')">Block </a>
                                        <%}else{%>
                                            <a href="/admin/users/userStatusUnblock?id=<%=user[i]._id%>" class="btn btn-sm btn-brand rounded font-sm mt-15" onclick="return confirmDelete('/admin/users/userStatusUnblock?id=<%=user[i]._id%>','unblock')">Unblock</a>
                                            <%}%>
                                    </td>
                                </tr>
                                <%}%>
                                
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
