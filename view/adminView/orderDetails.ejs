        <section class="content-main">
            <div class="container">
                <div class="card">
                    <% if (order.return) { %>
                        <div class="card-header">
                            <h4 class="card-title">Return Request</h4>
                        </div>
                        <div class="card-body">
                            <p class="card-text">Order ID: <%= order.orderId %></p>
                            <p class="card-text">Reason: <%= order.returnReason %></p>
                            <div class="btn-group" role="group" aria-label="Return Actions">
                                <a href="/admin/approveReturn/<%=order._id%>" class="btn btn-success">Approve</a>
                                <a href="/admin/rejectReturn/<%=order._id%>" class="btn btn-danger">Reject</a>
                            </div>
                        </div>
                    <% } %>
                </div>
                
                
                <div class="row mt-4">
                    <form action="/admin/updateOrder" method="post">
                    <div class="row">
                        <div class="col-md-6">
                            <h4>Order Id:<%=order.orderId%></h4>
                        </div>
                        <div class="col-md-3">
                            <h5>Ordered Date: <%=orderedDate%></h4>
                                <%if(order.deliveryStatus){%>
                                    <h5>Delivery Date : <%=deliveryDate%></h5>
                                    <%}else{%>
                                        <h5>Delivery Date : Nil</h5>
                                        <%}%>
                        </div>
                        <div class="col-md-3">
                            <label for="orderStatus"><h4>Change Status</h4> </label>
                            <select class="form-select " name="orderStatus"  id="orderStatus"  >
                                
                                <option value="pending" >Pending</option>
                                <option value="Cancelled" >Cancelled</option>
                                <option value="shipped" >Shipped</option>
                                <option value="delivered" >Delivered</option>
                            </select>
                        </div>
                    </div>
                    
                    
                        <!-- Card for Address, Order Summary, and Payment Method -->
                    <div class="card mt-10 mb-30">
                            
                        <div class="pt-20 pb-50 m-10">
                            <div class="row">
                                <div class="col-md-3 ">
                                    <div><h5 class="card-title">Delivery Address</h5></div>
                                    <p class="card-text"><%=addressData.name%><br><%=addressData.houseName%>,<br><%=addressData.street%>, <%=addressData.city%>,<br><%=addressData.district%>, <%=addressData.state%> <br> <%=addressData.pin%> </p>
                                </div>

                                <div class="col-md-3">
                                    
                                    <h5 class="card-title">Payment Method</h5>
                                    <p class="card-text"><%=order.paymentMethod%></p>
                                </div>

                                <div class="col-md-3">
                                    
                                    <h5 class="card-title">Status</h5>

                                    <% if (order.status == 'delivered') { %>
                                        <span class="badge rounded-pill alert-success" style="font-size: 18px;">Delivered</span>
                                    <% } else if (order.status == 'pending') { %>
                                        <span class="badge rounded-pill alert-warning" style="font-size: 18px;">Pending</span>
                                    <% } else if (order.status == 'Cancelled') { %>
                                        <span class="badge rounded-pill alert-danger" style="font-size: 18px;">Cancelled</span>
                                    <% } else if (order.status == 'returned') { %>
                                        <span class="badge rounded-pill alert-primary" style="font-size: 18px;">Returned</span>
                                    <% } else { %>
                                        <%= order.status %>
                                    <% } %>

                                   
                                    
                                    
                                </div>

                                <div class="col-md-3">
                                    <div class="row"><h5 class="card-title">Order Summary</h5></div>
                                    <div class="row">
                                        <div class="col-md-4"><p>Subtotal</p></div>
                                        <div class="col-md-8"><p> : ₹<%=order.total%>.00</p></div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4"><p>Shipping</p></div>
                                        <div class="col-md-8 "><p> : ₹0.00</p></div>
                                    </div>
                                    <div class="row">
