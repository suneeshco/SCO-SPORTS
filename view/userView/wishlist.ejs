        <section class="mt-50 mb-50">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            <%if(productData.length<1){%>
                                <p style="color: red;">No Items Yet...</p>
                            <%}else{%> 
                            <table class="table shopping-summery text-center">
                                <thead>
                                    <tr class="main-heading">
                                        <th scope="col" colspan="2">Product</th>
                                        <th scope="col">Price</th>
                                        <th scope="col">Stock Status</th>
                                        <th scope="col">Action</th>
                                        <th scope="col">Remove</th>
                                    </tr>
                                </thead>
                                
                                <tbody>
                                    <%for(let i=0;i< productData.length;i++){%>
                                        <form action="/addToCartFromWishlist" method="post">
                                    <tr>
                                        <td class="image product-thumbnail"><img src="/admin-assets/imgs/croppedProducts/<%=productData[i].images[0].url%>" alt="#"></td>
                                        <td class="product-des product-name">
                                            <h5 class="product-name"><a href="/products/productsDetail?id=<%=productData[i]._id%>"><%=productData[i].title%></a></h5>
                                            <p class="font-xs">
                                                <%=productData[i].category%>
                                            </p>
                                        </td>
                                        <td class="price" data-title="Price"><span>₹<%=productData[i].offerPrice%></span></td>
                                        <td class="text-center" data-title="Stock">
                                            <%if(productData[i].stock< 1){%>
                                                <span class="color3 font-weight-bold">Out of Stock</span>
                                            <%}else{%>
                                            <span class="color3 font-weight-bold">In Stock</span>
                                            <%}%>
                                        </td>
                                        
                                        <input type="hidden" value="<%=productData[i]._id%>" name="productId">
                                        <input type="hidden" value="<%=userData._id%>" name="userId">
                                        <input type="hidden" name="size" value="<%= productData[i].size %>">
                                        <td class="text-right" data-title="Cart">
                                            <button class="btn btn-sm"><i class="fi-rs-shopping-bag mr-5"></i>Add to cart</button>
                                        </td>
                                        <td class="action" data-title="Remove"><a href="/wishlistDelete/<%=userData.wishlist[i]._id%>"><i class="fi-rs-trash"></i></a></td>
                                    </tr>
                                </form>
                                    <%}%>
                                    
                                </tbody>
                           
                            </table>
                            <%}%>
                        </div>
                    </div>
                </div>
            </div>
